{
  "name": "components-angular",
  "version": "0.0.0",
  "scripts": {
    "serve": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "test:spec": "jest",
    "test:spec:watch": "jest --watch",
    "test:e2e": "start-test serve http://localhost:4200 '$(npm bin)/cypress run'",
    "test:e2e:watch": "start-test serve http://localhost:4200 '$(npm bin)/cypress open'"
  },
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "~13.4.0",
    "@angular/common": "~13.4.0",
    "@angular/compiler": "~13.4.0",
    "@angular/core": "~13.4.0",
    "@angular/elements": "^13.4.0",
    "@angular/forms": "13.4.0",
    "@angular/platform-browser": "~13.4.0",
    "@angular/platform-browser-dynamic": "~13.4.0",
    "@angular/router": "~13.4.0",
    "fuse.js": "^6.4.6",
    "popmotion-pose": "^3.4.11",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.3.11",
    "@angular/compiler-cli": "~13.4.0",
    "@angular/language-service": "~13.4.0",
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "codelyzer": "^6.0.0",
    "cypress": "^5.6.0",
    "eslint": "^8.17.0",
    "husky": "^6.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^13.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.12.1",
    "ts-node": "~10.1.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.6.4"
  },
  "peerDependencies": {},
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/angular@9.0.14"
    },
    "componentsDefaultDirectory": "src/app/components/{name}",
    "packageManager": "yarn",
    "saveDependenciesAsComponents": true,
    "overrides": {
      "*": {
        "peerDependencies": {
          "@angular/common": ">=8.0.0",
          "@angular/compiler": ">=8.0.0",
          "@angular/compiler-cli": ">=8.0.0",
          "zone.js": ">=0.10.0"
        }
      },
      "error": {
        "dependencies": {
          "@angular/animations": "-"
        },
        "peerDependencies": {
          "@angular/animations": ">=8.0.0"
        }
      },
      "load-more": {
        "dependencies": {
          "rxjs": "-"
        },
        "peerDependencies": {
          "rxjs": ">=6.0.0"
        },
        "propagate": true
      },
      "cart": {
        "dependencies": {
          "rxjs": "-",
          "popmotion-pose": "-"
        },
        "peerDependencies": {
          "rxjs": ">=6.0.0",
          "popmotion-pose": ">=3.4.0"
        },
        "propagate": true
      },
      "search": {
        "dependencies": {
          "@angular/forms": "-",
          "rxjs": "-",
          "popmotion-pose": "-"
        },
        "peerDependencies": {
          "@angular/forms": ">=8.0.0",
          "rxjs": ">=6.0.0",
          "popmotion-pose": ">=3.4.0"
        },
        "propagate": true
      },
      "slide": {
        "dependencies": {
          "rxjs": "-"
        },
        "peerDependencies": {
          "rxjs": ">=6.0.0"
        },
        "propagate": true
      },
      "shared": {
        "dependencies": {
          "@angular/animations": "-"
        },
        "peerDependencies": {
          "@angular/animations": ">=8.0.0"
        },
        "propagate": true
      },
      "sass": {
        "env": {
          "compiler": "-"
        }
      }
    }
  },
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts"
    }
  }
}
