{
  "name": "components-angular",
  "version": "0.0.0",
  "scripts": {
    "serve": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "test:spec": "jest",
    "test:spec:watch": "jest --watch",
    "test:e2e": "start-test serve http://localhost:4200 '$(npm bin)/cypress run'",
    "test:e2e:watch": "start-test serve http://localhost:4200 '$(npm bin)/cypress open'"
  },
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "~10.1.5",
    "@angular/common": "~10.1.5",
    "@angular/compiler": "~10.1.5",
    "@angular/core": "~10.1.5",
    "@angular/elements": "^10.1.5",
    "@angular/forms": "10.1.5",
    "@angular/platform-browser": "~10.1.5",
    "@angular/platform-browser-dynamic": "~10.1.5",
    "@angular/router": "~10.1.5",
    "fuse.js": "^6.4.4",
    "popmotion-pose": "^3.4.11",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.1.5",
    "@angular/language-service": "~11.0.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "codelyzer": "^6.0.1",
    "cypress": "^5.6.0",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-preset-angular": "^8.3.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.11.7",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.5"
  },
  "peerDependencies": {
    "@angular/core": ">=8.0.0",
    "@angular/elements": ">=8.0.0",
    "@angular/platform-browser": ">=8.0.0"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/angular@9.0.14"
    },
    "componentsDefaultDirectory": "src/app/components/{name}",
    "packageManager": "yarn",
    "saveDependenciesAsComponents": true,
    "overrides": {
      "*": {
        "peerDependencies": {
          "@angular/common": ">=8.0.0",
          "@angular/compiler": ">=8.0.0",
          "@angular/compiler-cli": ">=8.0.0",
          "zone.js": ">=0.10.0"
        }
      },
      "error": {
        "dependencies": {
          "@angular/animations": "-"
        },
        "peerDependencies": {
          "@angular/animations": ">=8.0.0"
        }
      },
      "load-more": {
        "dependencies": {
          "rxjs": "-"
        },
        "peerDependencies": {
          "rxjs": ">=6.0.0"
        },
        "propagate": true
      },
      "cart": {
        "dependencies": {
          "rxjs": "-",
          "popmotion-pose": "-"
        },
        "peerDependencies": {
          "rxjs": ">=6.0.0",
          "popmotion-pose": ">=3.4.0"
        },
        "propagate": true
      },
      "search": {
        "dependencies": {
          "@angular/forms": "-",
          "rxjs": "-",
          "popmotion-pose": "-"
        },
        "peerDependencies": {
          "@angular/forms": ">=8.0.0",
          "rxjs": ">=6.0.0",
          "popmotion-pose": ">=3.4.0"
        },
        "propagate": true
      },
      "slide": {
        "dependencies": {
          "rxjs": "-"
        },
        "peerDependencies": {
          "rxjs": ">=6.0.0"
        },
        "propagate": true
      },
      "shared": {
        "dependencies": {
          "@angular/animations": "-"
        },
        "peerDependencies": {
          "@angular/animations": ">=8.0.0"
        },
        "propagate": true
      },
      "sass": {
        "env": {
          "compiler": "-"
        }
      }
    }
  },
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts"
    }
  }
}
